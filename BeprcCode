package spec;

import com.codeborne.selenide.WebDriverRunner;
import org.junit.jupiter.api.Disabled;
import org.junit.jupiter.api.DisplayName;
import org.junit.jupiter.api.Test;
import org.openqa.selenium.By;

import static com.codeborne.selenide.Condition.text;
import static com.codeborne.selenide.Condition.visible;
import static com.codeborne.selenide.Selectors.by;
import static com.codeborne.selenide.Selectors.byText;
import static com.codeborne.selenide.Selenide.$;
import static com.codeborne.selenide.Selenide.open;
import static com.codeborne.selenide.WebDriverRunner.closeWindow;
import static java.lang.Thread.sleep;
import static org.junit.jupiter.api.Assertions.assertEquals;



public class BeprcLogin {

   // BeprcPages beprcPages=new BeprcPages();


    @Test
    @DisplayName("Super Admin Login and Navbar Check")
    public void superAdmin() throws Exception {
        ScreenRecorderUtil.startRecord("Super Admin Login , Role Check and Navbar Check");

        open("http://localhost:5173/");
        sleep(200);
        //Login / Register
        $(byText("Login / Register")).shouldBe(visible).click();
        sleep(200);
//        $(By.className("bg-orange-600")).shouldBe(visible).click();
        $(By.id("username")).setValue("superadmin@beprc.com");
        $(By.id("password")).setValue("12345");
        sleep(200);
        $(By.name("login")).click();
        sleep(200);
        //$(By.className("text-sm")).shouldHave(text("SUPER_ADMIN"));
       $(".text-sm.text-gray-200").shouldHave(text("Super Admin"));
        String elementText = $(".text-sm.text-gray-200").getText();
        System.out.println("Element text Role Name : " + elementText);
        sleep(500);

//      href='/
        String sliderText = $("a[href='/']").text();
        System.out.println(sliderText);
        assertEquals("Slider Image", sliderText);
        $("a[href='/']").click();
        sleep(200);
        String urlSliderImage = WebDriverRunner.url();
        assertEquals(urlSliderImage, "http://localhost:5173/");
        sleep(200);
//      href='/news'
        String newsText = $("a[href='/news']")
                .shouldBe(visible)
                .getText();
        assertEquals("Bulletin", newsText);
        System.out.println(newsText);
        $("a[href='/news']").click();
        sleep(200);
        String urlNews = WebDriverRunner.url();
        assertEquals(urlNews, "http://localhost:5173/news");
        sleep(200);
//      href='/activities
        String activitesText = $("a[href='/activities']").text();
        System.out.println(activitesText);
        assertEquals("Key Activities", activitesText);
        $("a[href='/activities']").click();
        sleep(200);
        String urlActivities = WebDriverRunner.url();
        assertEquals(urlActivities, "http://localhost:5173/activities");
        sleep(200);
//      href="/messages"
        String messagesText = $("a[href='/messages']").text();
        System.out.println(messagesText);
        assertEquals("Message", messagesText);
        $("a[href='/messages']").click();
        sleep(200);
        String urlMessages = WebDriverRunner.url();
        assertEquals(urlMessages, "http://localhost:5173/messages");
        sleep(200);
//      href="/fundings
        String fundingsText = $("a[href='/fundings']").text();
        System.out.println(fundingsText);
        assertEquals("About BEPRC", fundingsText);
        $("a[href='/fundings']").click();
        sleep(200);
        String urlFundings = WebDriverRunner.url();
        assertEquals(urlFundings, "http://localhost:5173/fundings");
        sleep(200);
//      href="/articles
        String articlesText = $("a[href='/articles']").text();
        System.out.println(articlesText);
        assertEquals("News and Articles", articlesText);
        $("a[href='/articles']").click();
        sleep(200);
        String urlArticles = WebDriverRunner.url();
        assertEquals(urlArticles, "http://localhost:5173/articles");
        sleep(200);

//      href="/important-links
        String importantLinksText = $("a[href='/important-links']").text();
        System.out.println(importantLinksText);
        assertEquals("Important Link", importantLinksText);
        $("a[href='/important-links']").click();
        sleep(200);
        String urlImportantLinks = WebDriverRunner.url();
        assertEquals(urlImportantLinks, "http://localhost:5173/important-links");
        sleep(200);
//      href="/admin-users"
        String adminUsersText = $("a[href='/admin-users']").text();
        System.out.println(adminUsersText);
        assertEquals("Wing Admin Assign", adminUsersText);
        $("a[href='/admin-users']").click();
        sleep(200);
        String urlAdminUsers = WebDriverRunner.url();
        assertEquals(urlAdminUsers, "http://localhost:5173/admin-users");
        sleep(200);
//      href="/guidlines"
        String guidlinesText = $("a[href='/guidlines']").text();
        System.out.println(guidlinesText);
        assertEquals("Guideline", guidlinesText);
        $("a[href='/guidlines']").click();
        sleep(200);
        String urlGuideline = WebDriverRunner.url();
        assertEquals(urlGuideline, "http://localhost:5173/guidlines");
        $(By.className("fill-gray-100")).shouldBe(visible).click();
        $(byText("Log Out")).shouldBe(visible).click();
        closeWindow();

        sleep(500);
        ScreenRecorderUtil.stopRecord();
    }

    @Test
    @DisplayName("Innovation Admin Login and Navbar Check")
    public void innovationAdminLogin() throws Exception {
        ScreenRecorderUtil.startRecord("Innovation Admin Login, Role Check and Navbar Check");

        open("http://localhost:5173/");
        sleep(200);
        $(byText("Login / Register")).shouldBe(visible).click();


        sleep(200);
        $(By.id("username")).setValue("innovationAdmin@beprc.com");
        $(By.id("password")).setValue("12345");
        $(By.name("login")).click();
        sleep(200);

        $(".text-sm.text-gray-200").shouldHave(text("Innovation Admin"));
        String elementText = $(".text-sm.text-gray-200").getText();
        System.out.println("Element text Role Name : " + elementText);
        //$(byText("INNOVATION_ADMIN")).shouldHave(text("INNOVATION_ADMIN"));
        sleep(200);

//      href="/dashboard"
        String dashboardText = $("a[href='/dashboard']").text();
        System.out.println(dashboardText);
        assertEquals("Dashboard", dashboardText);
        $("a[href='/dashboard']").click();
        sleep(200);
        String urlDashboard = WebDriverRunner.url();
        assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//      href="/proposal"
        String proposalText = $("a[href='/proposal']").text();
        System.out.println(proposalText);
        assertEquals("Proposal", proposalText);
        $("a[href='/proposal']").click();
        sleep(200);
        String urlProposal = WebDriverRunner.url();
        assertEquals(urlProposal, "http://localhost:5173/proposal");

//      href="/project-management-admin/14a5701a-f3cb-453e-a227-dcf66f95e2cf"
        String projectManagementAdminText = $("a[href='/project-management-admin/14a5701a-f3cb-453e-a227-dcf66f95e2cf']").text();
        System.out.println(projectManagementAdminText);
        assertEquals("Project Management", projectManagementAdminText);
        $("a[href='/project-management-admin/14a5701a-f3cb-453e-a227-dcf66f95e2cf']").click();
        sleep(200);
        String urlProjectManagement = WebDriverRunner.url();
        assertEquals(urlProjectManagement, "http://localhost:5173/project-management-admin/14a5701a-f3cb-453e-a227-dcf66f95e2cf");

//      href="/report-management-admin/14a5701a-f3cb-453e-a227-dcf66f95e2cf"
        String reportManagementAdminText = $("a[href='/report-management-admin/14a5701a-f3cb-453e-a227-dcf66f95e2cf']").text();
        System.out.println(reportManagementAdminText);
        assertEquals("Report Management", reportManagementAdminText);
        $("a[href='/report-management-admin/14a5701a-f3cb-453e-a227-dcf66f95e2cf']").click();
        sleep(200);
        String urlReportManagement = WebDriverRunner.url();
