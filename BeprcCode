    @Test
    @DisplayName("Lab Negotiation Committee Login and Navbar Check")
    public void labNegotiationCommittee() throws InterruptedException {
        open("http://localhost:5173/");
        sleep(200);

        $(byText("Login / Register")).shouldBe(visible).click();
        sleep(200);

        $(By.id("username")).setValue("lbdNgCm1@beprc.com");
        $(By.id("password")).setValue("12345");
        $(By.name("login")).click();
        sleep(200);

        String elementText = $(By.cssSelector(".text-sm.text-gray-200")).shouldBe(visible).getText();
        boolean x =elementText.contains("Negotiation Committee");
        if(x){
            sleep(500);
//          href="/dashboard"
            String dashboardText = $("a[href='/dashboard']").text();
            System.out.println(dashboardText);
            assertEquals("Dashboard", dashboardText);
            $("a[href='/dashboard']").click();
            sleep(200);
            String urlDashboard = WebDriverRunner.url();
            assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//          href="/proposal"
            String proposalText = $("a[href='/proposal']").text();
            System.out.println(proposalText);
            assertEquals("Proposal", proposalText);
            $("a[href='/proposal']").click();
            sleep(200);
            String urlProposal = WebDriverRunner.url();
            assertEquals(urlProposal, "http://localhost:5173/proposal");

        }else{
            $(By.className("fill-gray-100")).click();
            $(byText("Negotiation Committee")).shouldBe(visible).click();
            sleep(2000);

//          href="/dashboard"
            String dashboardText = $("a[href='/dashboard']").text();
            System.out.println(dashboardText);
            assertEquals("Dashboard", dashboardText);
            $("a[href='/dashboard']").click();
            sleep(200);
            String urlDashboard = WebDriverRunner.url();
            assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//          href="/proposal"
            String proposalText = $("a[href='/proposal']").text();
            System.out.println(proposalText);
            assertEquals("Proposal", proposalText);
            $("a[href='/proposal']").click();
            sleep(200);
            String urlProposal = WebDriverRunner.url();
            assertEquals(urlProposal, "http://localhost:5173/proposal");


        }
        $(By.className("fill-gray-100")).shouldBe(visible).click();
        $(byText("Log Out")).shouldBe(visible).click();
        closeWindow();
        sleep(1000);
        //ScreenRecorderUtil.stopRecord();

    }

