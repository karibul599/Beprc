        $(By.className("fill-gray-100")).shouldBe(visible).click();
        $(byText("Log Out")).shouldBe(visible).click();
        closeWindow();
        sleep(500);
        ScreenRecorderUtil.stopRecord();
    }

    @Test
    @DisplayName("Innovation Screening Committee Login and Navbar Check")
    public void innovationScreeningCommittee() throws InterruptedException {
        open("http://localhost:5173/");
        sleep(200);

        $(byText("Login / Register")).shouldBe(visible).click();
        sleep(200);

        $(By.id("username")).setValue("innovationScreeningCommittee1@beprc.com");
        $(By.id("password")).setValue("12345");
        $(By.name("login")).click();
        sleep(200);

        String elementText = $(By.cssSelector(".text-sm.text-gray-200")).shouldBe(visible).getText();
        boolean x =elementText.contains("Innovation Screening Committee");
        if(x){
            sleep(500);
//          href="/dashboard"
            String dashboardText = $("a[href='/dashboard']").text();
            System.out.println(dashboardText);
            assertEquals("Dashboard", dashboardText);
            $("a[href='/dashboard']").click();
            sleep(200);
            String urlDashboard = WebDriverRunner.url();
            assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//          href="/proposal"
            String proposalText = $("a[href='/proposal']").text();
            System.out.println(proposalText);
            assertEquals("Proposal", proposalText);
            $("a[href='/proposal']").click();
            sleep(200);
            String urlProposal = WebDriverRunner.url();
            assertEquals(urlProposal, "http://localhost:5173/proposal");

        }else{
            $(By.className("fill-gray-100")).click();
            $(byText("Innovation Screening Committee")).shouldBe(visible).click();
            sleep(2000);

//          href="/dashboard"
            String dashboardText = $("a[href='/dashboard']").text();
            System.out.println(dashboardText);
            assertEquals("Dashboard", dashboardText);
            $("a[href='/dashboard']").click();
            sleep(200);
            String urlDashboard = WebDriverRunner.url();
            assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//          href="/proposal"
            String proposalText = $("a[href='/proposal']").text();
            System.out.println(proposalText);
            assertEquals("Proposal", proposalText);
            $("a[href='/proposal']").click();
            sleep(200);
            String urlProposal = WebDriverRunner.url();
            assertEquals(urlProposal, "http://localhost:5173/proposal");


        }
        $(By.className("fill-gray-100")).shouldBe(visible).click();
        $(byText("Log Out")).shouldBe(visible).click();
        closeWindow();
        sleep(1000);
        //ScreenRecorderUtil.stopRecord();
        sleep(500);

    }

    @Test
    @DisplayName("innovation Reviewer Login and Navbar Check")
    public void innovationReviewer() throws InterruptedException {
        sleep(1000);
        //ScreenRecorderUtil.startRecord("INNOVATION_REVIEWER_1 Check Ro  le and Navbar");

        open("http://localhost:5173/");
        sleep(200);

        $(byText("Login / Register")).shouldBe(visible).click();
//        $(By.className("bg-orange-600"))
//                .shouldBe(visible)
//                .click();

        sleep(200);
        $(By.id("username")).setValue("innovationReviewer1@beprc.com");
        $(By.id("password")).setValue("12345");
        $(By.name("login")).click();
        $(byText("INNOVATION_REVIEWER_1")).shouldBe(visible).click();
        $(byText("Profile")).shouldBe(visible).click();


       /* String text = $("#name").val();
        System.out.println("Value of the element: " + text);
        assertEquals("INNOVATION_REVIEWER_1", text);

        sleep(1000);

        String text1 = $("#emailAddress").val();
        System.out.println("Value of the element: " + text1);
        assertEquals("innovationReviewer1@beprc.com", text1);

        sleep(5000);*/



        if($(".text-sm.text-gray-200").getText().contains("Reviewer")){
//          href="/dashboard"
            String dashboardText = $("a[href='/dashboard']").text();
            System.out.println(dashboardText);
            assertEquals("Dashboard", dashboardText);
            $("a[href='/dashboard']").click();
            sleep(200);
            String urlDashboard = WebDriverRunner.url();
            assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//          href="/proposal"
            String proposalText = $("a[href='/proposal']").text();
            System.out.println(proposalText);
            assertEquals("Proposal", proposalText);
            $("a[href='/proposal']").click();
            sleep(200);
            String urlProposal = WebDriverRunner.url();
            assertEquals(urlProposal, "http://localhost:5173/proposal");

        }else{
            $(By.className("fill-gray-100")).click();
              $(byText("Reviewer")).shouldBe(visible).click();
            sleep(2000);

//          href="/dashboard"
            String dashboardText = $("a[href='/dashboard']").text();
            System.out.println(dashboardText);
            assertEquals("Dashboard", dashboardText);
            $("a[href='/dashboard']").click();
            sleep(200);
            String urlDashboard = WebDriverRunner.url();
            assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//          href="/proposal"
            String proposalText = $("a[href='/proposal']").text();
            System.out.println(proposalText);
            assertEquals("Proposal", proposalText);
            $("a[href='/proposal']").click();
            sleep(200);
            String urlProposal = WebDriverRunner.url();
            assertEquals(urlProposal, "http://localhost:5173/proposal");


        }
        $(By.className("fill-gray-100")).shouldBe(visible).click();
        $(byText("Log Out")).shouldBe(visible).click();
        closeWindow();
        sleep(1000);
        //ScreenRecorderUtil.stopRecord();
    }
    @Test
    @DisplayName("Innovation Negation Committee Login and Navbar Check")
    public void innovationNegationCommittee() throws InterruptedException {
        open("http://localhost:5173/");
        sleep(200);

        $(byText("Login / Register")).shouldBe(visible).click();
        sleep(200);

        $(By.id("username")).setValue("innovationNegotiationCommittee1@beprc.com");
        $(By.id("password")).setValue("12345");
        $(By.name("login")).click();
        sleep(200);


        String elementText = $(By.cssSelector(".text-sm.text-gray-200")).shouldBe(visible).getText();
        boolean x =elementText.contains("Innovation Negotiation Committee");
        if(x){
            sleep(500);
//          href="/dashboard"
            String dashboardText = $("a[href='/dashboard']").text();
            System.out.println(dashboardText);
            assertEquals("Dashboard", dashboardText);
            $("a[href='/dashboard']").click();
            sleep(200);
            String urlDashboard = WebDriverRunner.url();
            assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//          href="/proposal"
            String proposalText = $("a[href='/proposal']").text();
            System.out.println(proposalText);
            assertEquals("Proposal", proposalText);
            $("a[href='/proposal']").click();
            sleep(200);
            String urlProposal = WebDriverRunner.url();
            assertEquals(urlProposal, "http://localhost:5173/proposal");

        }else{
            $(By.className("fill-gray-100")).click();
            $(byText("Innovation Negotiation Committee")).shouldBe(visible).click();
            sleep(2000);

//          href="/dashboard"
            String dashboardText = $("a[href='/dashboard']").text();
            System.out.println(dashboardText);
            assertEquals("Dashboard", dashboardText);
            $("a[href='/dashboard']").click();
            sleep(200);
            String urlDashboard = WebDriverRunner.url();
            assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//          href="/proposal"
            String proposalText = $("a[href='/proposal']").text();
            System.out.println(proposalText);
            assertEquals("Proposal", proposalText);
            $("a[href='/proposal']").click();
            sleep(200);
            String urlProposal = WebDriverRunner.url();
            assertEquals(urlProposal, "http://localhost:5173/proposal");


        }
        $(By.className("fill-gray-100")).shouldBe(visible).click();
        $(byText("Log Out")).shouldBe(visible).click();
        closeWindow();
        sleep(1000);
        //ScreenRecorderUtil.stopRecord();

    }


    @Test
    @DisplayName("Incubation Admin Login and Navbar Check")
    public void incubationAdmin() throws Exception {
        //ScreenRecorderUtil.startRecord("Incubation Admin Login and Navbar check");

        open("http://localhost:5173/");
        sleep(200);
        $(byText("Login / Register")).shouldBe(visible).click();
        /*  $(By.className("bg-orange-600")).shouldBe(visible).click();*/

        $(By.id("username")).setValue("incadm@ovi.com");
        $(By.id("password")).setValue("12345");
        sleep(200);
        $(By.name("login")).click();
        sleep(200);

        //$(byText("Incubation Admin")).shouldHave(text("Incubation Admin"));
        $(".text-sm.text-gray-200").shouldHave(text("Incubation Admin"));
        String elementText = $(".text-sm.text-gray-200").getText();
        System.out.println("Element text Role Name : " + elementText);
        sleep(200);

//      href="/dashboard"
        String dashboardText = $("a[href='/dashboard']").text();
        System.out.println(dashboardText);
        assertEquals("Dashboard", dashboardText);
        $("a[href='/dashboard']").click();
        sleep(200);
        String urlDashboard = WebDriverRunner.url();
        assertEquals(urlDashboard, "http://localhost:5173/dashboard");

//      href="/proposal"
        String proposalText = $("a[href='/proposal']").text();
        System.out.println(proposalText);
        assertEquals("Proposal", proposalText);
        $("a[href='/proposal']").click();
        sleep(200);
        String urlProposal = WebDriverRunner.url();
        assertEquals(urlProposal, "http://localhost:5173/proposal");

//      href="/project-management-admin/28071f83-77dd-40a9-8f68-1d622e6d4223"
        String projectManagementAdminText = $("a[href='/project-management-admin/28071f83-77dd-40a9-8f68-1d622e6d4223']").text();
        System.out.println(projectManagementAdminText);
        assertEquals("Project Management", projectManagementAdminText);
        $("a[href='/project-management-admin/28071f83-77dd-40a9-8f68-1d622e6d4223']").click();
        sleep(200);
        String urlProjectManagement = WebDriverRunner.url();
        assertEquals(urlProjectManagement, "http://localhost:5173/project-management-admin/28071f83-77dd-40a9-8f68-1d622e6d4223");

//      href="/report-management-admin/28071f83-77dd-40a9-8f68-1d622e6d4223"
        String reportManagementAdminText = $("a[href='/report-management-admin/28071f83-77dd-40a9-8f68-1d622e6d4223']").text();
        System.out.println(reportManagementAdminText);
        assertEquals("Report Management", reportManagementAdminText);
        $("a[href='/report-management-admin/28071f83-77dd-40a9-8f68-1d622e6d4223']").click();
        sleep(200);
        String urlReportManagement = WebDriverRunner.url();
        assertEquals(urlReportManagement, "http://localhost:5173/report-management-admin/28071f83-77dd-40a9-8f68-1d622e6d4223");

//      href="/solicitation"
        String solicitationText = $("a[href='/solicitation']").text();
        System.out.println(solicitationText);
        assertEquals("Incubation Solicitation", solicitationText);
        $("a[href='/solicitation']").click();
        sleep(200);
        String IncubationSolicitation = WebDriverRunner.url();
        assertEquals(IncubationSolicitation, "http://localhost:5173/solicitation");

//      href="/screening/INCUBATION_SCREENING_COMMITTEE/3760d33b-241c-486b-937f-045453969183"
        String screeningText = $("a[href='/screening/INCUBATION_SCREENING_COMMITTEE/3760d33b-241c-486b-937f-045453969183']").text();
        System.out.println(screeningText);
        assertEquals("Incubation Screening Committee", screeningText);
        $("a[href='/screening/INCUBATION_SCREENING_COMMITTEE/3760d33b-241c-486b-937f-045453969183']").click();
        sleep(200);
        String urlIncubationScreeningCommittee = WebDriverRunner.url();
        assertEquals(urlIncubationScreeningCommittee, "http://localhost:5173/screening/INCUBATION_SCREENING_COMMITTEE/3760d33b-241c-486b-937f-045453969183");

//      href="/negotiation/INCUBATION_NEGOTIATION_COMMITTEE/314cf5f5-c0ad-410e-9fa4-0d17ab5b7290"
        String negotiationText = $("a[href='/negotiation/INCUBATION_NEGOTIATION_COMMITTEE/314cf5f5-c0ad-410e-9fa4-0d17ab5b7290']").text();
        System.out.println(negotiationText);
        assertEquals("Incubation Negotiation Committee", negotiationText);
        $("a[href='/negotiation/INCUBATION_NEGOTIATION_COMMITTEE/314cf5f5-c0ad-410e-9fa4-0d17ab5b7290']").click();
        sleep(200);
        String urlIncubationNegotiationCommittee = WebDriverRunner.url();
        assertEquals(urlIncubationNegotiationCommittee, "http://localhost:5173/negotiation/INCUBATION_NEGOTIATION_COMMITTEE/314cf5f5-c0ad-410e-9fa4-0d17ab5b7290");

//      href="/reviewer/INCUBATION"
        String reviewerText = $("a[href='/reviewer/INCUBATION']").text();
        System.out.println(reviewerText);
        assertEquals("IPSC (Incubation Proposal Scoring Committee)", reviewerText);
        $("a[href='/reviewer/INCUBATION']").click();
        sleep(200);
        String urlIncubationProposalScoringCommittee = WebDriverRunner.url();
        assertEquals(urlIncubationProposalScoringCommittee, "http://localhost:5173/reviewer/INCUBATION");

//      href="/user"
        String userText = $("a[href='/user']").text();
        System.out.println(userText);
        assertEquals("User", userText);
        $("a[href='/user']").click();
        sleep(200);
        String urlUser = WebDriverRunner.url();
        assertEquals(urlUser, "http://localhost:5173/user");

//      href="/report"
        String reportText = $("a[href='/report']").text();
        System.out.println(reportText);
        assertEquals("Report", reportText);
        $("a[href='/report']").click();
        sleep(200);
        String urlReport = WebDriverRunner.url();
        assertEquals(urlReport, "http://localhost:5173/report");

//       TODO PCR & Recommendation Report
         $(by("data-testid", "ExpandMoreIcon")).shouldBe(visible).click();




//      href="/AttachmentDeclaration"
        String AttachmentDeclarationText = $("a[href='/AttachmentDeclaration']").text();
        System.out.println(AttachmentDeclarationText);
        assertEquals("Attachment & Declaration", AttachmentDeclarationText);
        $("a[href='/AttachmentDeclaration']").click();
