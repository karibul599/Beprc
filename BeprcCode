        sleep(200);
        String urlEvaluationCommittee = WebDriverRunner.url();
        assertEquals(urlEvaluationCommittee, "http://localhost:5173/reviewer/LAB_DEVELOPMENT");


//      href="/negotiation/LAB_DEV_NEGOTIATION_COMMITTEE/4afa2e4c-44e1-4c5c-aecc-0877f75aa4e8"
        String negotiationText = $("a[href='/negotiation/LAB_DEV_NEGOTIATION_COMMITTEE/4afa2e4c-44e1-4c5c-aecc-0877f75aa4e8']").text();
        System.out.println(negotiationText);
        assertEquals("Negotiation Committee", negotiationText);
        $("a[href='/negotiation/LAB_DEV_NEGOTIATION_COMMITTEE/4afa2e4c-44e1-4c5c-aecc-0877f75aa4e8']").click();
        sleep(200);
        String urlNegotiationCommittee = WebDriverRunner.url();
        assertEquals(urlNegotiationCommittee, "http://localhost:5173/negotiation/LAB_DEV_NEGOTIATION_COMMITTEE/4afa2e4c-44e1-4c5c-aecc-0877f75aa4e8");


//      href="/AttachmentDeclaration"
        String attachmentDeclarationText = $("a[href='/AttachmentDeclaration']").text();
        System.out.println(attachmentDeclarationText);
        assertEquals("Attachment & Declaration", attachmentDeclarationText);
        $("a[href='/AttachmentDeclaration']").click();
        sleep(200);
        String urlAttachmentDeclaration = WebDriverRunner.url();
        assertEquals(urlAttachmentDeclaration, "http://localhost:5173/AttachmentDeclaration");


//      href="/user"
        String userText = $("a[href='/user']").text();
        System.out.println(userText);
        assertEquals("User", userText);
        $("a[href='/user']").click();
        sleep(200);
        String urlUser = WebDriverRunner.url();
        assertEquals(urlUser, "http://localhost:5173/user");

//      href="/report"
        String reportText = $("a[href='/report']").text();
        System.out.println(reportText);
        assertEquals("Report", reportText);
        $("a[href='/report']").click();
        sleep(200);
        String urlReport = WebDriverRunner.url();
        assertEquals(urlReport, "http://localhost:5173/report");

//      href="/pcr-report-list"
        String pcrText = $("a[href='/pcr-report-list']").text();
        System.out.println(pcrText);
        assertEquals("Lab Development PCR", pcrText);
        $("a[href='/pcr-report-list']").click();
        sleep(200);
        String urlLabDevelopmentPCR = WebDriverRunner.url();
        assertEquals(urlLabDevelopmentPCR, "http://localhost:5173/pcr-report-list");

        sleep(200);
        $(By.className("fill-gray-100")).shouldBe(visible).click();
        $(byText("Log Out")).shouldBe(visible).click();
        closeWindow();
        sleep(500);
       // ScreenRecorderUtil.stopRecord();

    }

