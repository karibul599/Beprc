        assertEquals(urlAttachmentDeclaration, "http://localhost:5173/AttachmentDeclaration");

//      href="/screening-committee-criteria"
        String screeningCommitteeCriteriaText = $("a[href='/screening-committee-criteria']").text();
        System.out.println(screeningCommitteeCriteriaText);
        assertEquals("Screening Committee Criteria", screeningCommitteeCriteriaText);
        $("a[href='/screening-committee-criteria']").click();
        sleep(200);
        String urlScreeningCommitteeCriteria = WebDriverRunner.url();
