        open("http://localhost:5173/");
        sleep(200);
        $(byText("Login / Register")).shouldBe(visible).click();
        /*$(By.className("bg-orange-600"))
                .shouldBe(visible)
                .click();*/

        sleep(200);
        $(By.id("username")).setValue("entadm@yahoo.com");
        $(By.id("password")).setValue("12345");
        $(By.name("login")).click();
        sleep(200);

        $(".text-sm.text-gray-200").shouldHave(text("Entrepreneurship Admin"));
